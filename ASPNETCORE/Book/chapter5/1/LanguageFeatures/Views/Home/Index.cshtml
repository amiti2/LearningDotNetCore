@model IEnumerable<string>
<!DOCTYPE html>
<html>
    <head>
        <title>Language Features</title>
        <meta name="viewport" content="width = device-width"/>
    </head>
    
    <body>
        <ul>
            @foreach(string s in Model)
            {
                <li>@s</li>
            }
        </ul>
    </body>
